{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Worker",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "env": {},
            "args": [
                "tcp://192.168.0.107:5557", 
                "tcp://192.168.0.107:5558"
            ]
        },
        {
            "name": "Run and Dump",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "env": {},
            "args": [
                "tcp://192.168.0.107:5557", 
                "-no-push",
                "${workspaceFolder}/target/dump"
            ]
        }
    ]
}